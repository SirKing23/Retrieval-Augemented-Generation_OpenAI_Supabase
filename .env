
# OpenAI API Configuration
OPENAI_API_KEY="OpenAIKey"

# Supabase Configuration
SUPABASE_URL = "supabase_URL"
SUPABASE_KEY = "supabase_key"


# Model Configuration
MAIN_MODEL=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small

# Response Configuration
MAX_TOKEN_RESPONSE=700

# Vector Search Configuration
VECTOR_SEARCH_THRESHOLD=0.2
VECTOR_SEARCH_MATCH_COUNT=5

# Query Classification Configuration
CLASSIFICATION_MODEL=gpt-3.5-turbo
CLASSIFICATION_MAX_TOKENS=20
CLASSIFICATION_TEMPERATURE=0.1
CLASSIFICATION_CONFIDENCE_THRESHOLD=0.7

# Chunking Configuration
MAX_CHUNK_TOKENS=600
OVERLAP_CHUNK_TOKENS=300

# Performance Configuration
ENABLE_ASYNC=false
LOG_LEVEL=INFO
CACHE_SIZE=1000

# Security Configuration
MAX_INPUT_LENGTH=10000
SANITIZE_INPUT=true


# Cache Directory
CACHE_DIR=./src/server/cache
#Knowledge base directory
DOCUMENTS_DIR=./data/Knowledge_Base_Files
PROCESSED_FILES_DIR=./data/processed_files.txt

